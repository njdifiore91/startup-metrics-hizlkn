{
  "name": "startup-metrics-benchmarking-backend",
  "version": "1.0.0",
  "description": "Backend service for the Startup Metrics Benchmarking Platform",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "npm audit && snyk test",
    "test:performance": "artillery run tests/performance/load-test.yml",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "docs:generate": "swagger-jsdoc -d swaggerDef.js -o swagger.json",
    "docs:serve": "swagger-ui-express serve swagger.json",
    "docker:build": "docker build -t startup-metrics-backend .",
    "docker:run": "docker run -p 3000:3000 startup-metrics-backend",
    "aws:deploy": "aws-deploy.sh",
    "db:backup": "pg-backup.sh",
    "prepare": "cd ../../ && husky install",
    "commit": "git-cz"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.32.0",
    "@aws-sdk/client-s3": "^3.32.0",
    "@aws-sdk/client-ses": "^3.0.0",
    "big.js": "^6.2.1",
    "cache-manager": "^5.2.0",
    "chart.js": "^4.0.0",
    "compression": "^1.7.4",
    "correlation-id": "^5.2.0",
    "cors": "^2.8.5",
    "csv-stringify": "^6.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-cache-controller": "^1.1.0",
    "express-correlation-id": "^3.0.1",
    "express-rate-limit": "^6.7.0",
    "fs-extra": "^11.0.0",
    "google-auth-library": "^8.7.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.0",
    "jsonwebtoken": "^9.0.0",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.0",
    "prom-client": "^14.0.0",
    "rate-limiter-flexible": "^2.3.1",
    "redis": "^4.6.7",
    "sanitize-html": "^2.11.0",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.0",
    "stoppable": "^1.1.0",
    "swagger-ui-express": "^4.6.3",
    "tsyringe": "^4.7.0",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/express-correlation-id": "^1.2.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "husky": "^8.0.3",
    "ioredis-mock": "^8.2.2",
    "jest": "^29.5.0",
    "nock": "^13.3.1",
    "nodemon": "^3.1.9",
    "prettier": "^2.8.0",
    "redis-mock": "^0.56.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
